      title: 'SPICE 配信開始！ | SUSHIBOYS',
      description: 'SPICE (feat. FARMHOUSE) / PARKGOLF',
      url: 'https://sushiboys350.com/news/post005/',
      pageid: 'news',
      index: 4,
      path: '../../'
    };
    const localogp = {
      title: localmeta.title,
      description: localmeta.description,
      url: localmeta.url
    };
    const localfile = {
      css: '/css/news-detail.css',
