<!DOCTYPE html>
<html lang="ja">
<head>
  <%_
    const localmeta = {
      title: `高速道路 | SUSHIBOYS`,
      description: `高速道路`,
      url: `https://sushiboys350.com/discography/single/kousokudouro.html`,
      pageid: `discography`,
      type: `single`,
      index: 2,
      path: `../../`
    };
    const localogp = {
      title: localmeta.title,
      description: localmeta.description,
      url: localmeta.url
    };
    const localfile = {
      css: `/css/discography-detail.css`,
      js: `/js/discographyDetail.js`
    };
  _%>
  <%- include(localmeta.path + `_inc/_head`, {meta: localmeta, ogp: localogp, file: localfile}) %>
  <%- include(localmeta.path + `_data/_discography`); %>
  <%_ discographyData = discography(); _%>
</head>
<body>
  <%- include(localmeta.path + `_inc/components/_googletagmanager`); %>
  <div
    id="wrapper"
    class="wrapper"
  >
    <%- include(localmeta.path + `_inc/components/_header`, {meta: localmeta}); %>
    <main class="main">
      <%- include(localmeta.path + `_inc/components/_hero`, {meta: localmeta}); %>
      <div class="content">
        <%- include(localmeta.path + `_inc/components/discography/detail/_article`, {meta: localmeta, discographyData: discographyData}); %>
      </div>
    </main>
    <%- include(localmeta.path + `_inc/components/_toggle`); %>
    <%- include(localmeta.path + `_inc/components/_footer`); %>
  </div>
</body>
</html>
