<!DOCTYPE html>
<html lang="ja">
<head>
  <%- include('../_data/_goods'); %>
  <%
  const localmeta = {
    title: 'GOODS | SUSHIBOYS',
    description: 'SUSHIBOYSのグッズ一覧。グッズ購入・商品詳細ページへご案内いたします。',
    url: 'https://sushiboys350.com/goods/',
    pageid: 'goods',
    path: '../'
  };
  const localogp = {
    title: localmeta.title,
    description: localmeta.description,
    url: localmeta.url
  };
  const localfile = {
    css: '/css/goods.css',
    js: '/js/goods.js'
  };
  const goodsData = goods();
  %>
  <%- include(localmeta.path + '_inc/_head', {meta: localmeta, ogp: localogp, file: localfile}); %>
</head>
<body>
  <%- include(localmeta.path + '_inc/components/_googletagmanager'); %>
  <div class="wrapper">
    <%- include(localmeta.path + '_inc/components/_header', {meta: localmeta}); %>
    <main class="main">
      <%- include(localmeta.path + '_inc/components/_heading', {meta: localmeta}); %>
      <%- include(localmeta.path + '_inc/components/goods/_goods', {goodsData: goodsData}); %>
    </main>
    <%- include(localmeta.path + '_inc/components/_footer'); %>
  </div>
</body>
</html>
