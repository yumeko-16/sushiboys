{
  "htmlWhitespaceSensitivity": "strict",
  "singleAttributePerLine": true,
  "overrides": [
    {
      "files": "*.js",
      "options": {
        "singleQuote": true
      }
    },
    {
      "files": "*.ejs",
      "options": {
        "parser": "html",
        "embeddedLanguageFormatting": "off"
      }
    }
  ]
}
